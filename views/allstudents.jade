extends layout

block content
  h1 All Students
  table(border="1")
    tr
      th First Name
      th Last Name
      th Country
      th Major
      th Entry
    -for(let i=0;i<students.length;i++){
    tr
      td=students[i].firstName
      td=students[i].lastName
      td=students[i].country
      td=students[i].major
      td=students[i].entry
    -}